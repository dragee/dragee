{
  "presets": [
    "@babel/env"
  ],
  "env": {
    "development": {
      "presets": [
        [
          "@babel/env", {
            targets: { browsers: "> 0.25%, not dead, ie >= 11" },
            modules: false,
            spec: true,
            useBuiltIns: "entry",
            forceAllTransforms: true,
            corejs: {
              version: 3,
              proposals: false
            }
          }
        ]
      ]
    },
    "test": {
      "presets": [
        ["@babel/env", {
          targets: {
            node: "current"
          }
        }]
      ]
    }
  }
}
